type dgraph.graphql{
    dgraph.graphql.schema
}

type Commune {
    name
    description
    created
    members
    address
    duties
}

type User {
    name
    created
    birth
}

type Address {
    created
    street
    city
    zip
}

type Duty {
    created
    changed
    name
    description
    lastDone
    nextDone
    rotationIndex
    rotationTime
    rotationList
}

name: string .
description: string .
created: int .
members: [uid] .
address : uid .
birth: int .
street: string .
city: string .
zip: string .
changed: int .
lastDone: int .
nextDone: int .
rotationTime: int .
rotationList: [uid] .
duties: [uid] .
rotationIndex: int .
dgraph.graphql.schema: string .
